{
  "name": "yuxi-global-new-site",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:ghpages": "ng build --prod --base-href https://angelitaooo.github.io/YuxiGlobal-NewSite/",
    "ghpages:github": "gh-pages -d dist -o github",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build:ghpages && npm run ghpages:github",
    "ngrok": "ngrok http --host-header=rewrite 4200",
    "build": "npm run build-en && npm run build-es",
    "build-en": "ng build --prod --aot -op dist -bh /",
    "copy-handler": "mkdir dist\\bin && copy src\\bin\\Yuxi.HttpModules.dll dist\\bin ",
    "copy-config": "copy src\\web.config dist ",
    "build-es": "ng build --prod --aot -op dist/es -bh /es/ --i18nFile=src/locale/messages.es.xlf --i18nFormat=xlf --locale=es",
    "image-optimization": "imagemin src/assets/images/* --out-dir=src/assets/images"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "gh-pages": "^1.0.0",
    "hammerjs": "^2.0.8",
    "ng-recaptcha": "^3.0.2",
    "ngx-carousel": "^1.3.2",
    "ngx-fullpage": "^4.0.6",
    "normalize.css": "^7.0.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "imagemin-cli": "^3.0.0",
    "gulp": "^3.9.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrok": "^2.2.21",
    "protractor": "~5.1.2",
    "run-sequence": "^2.2.1",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "2.4.2"
  }
}
